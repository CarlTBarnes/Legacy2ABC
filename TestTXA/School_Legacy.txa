[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Call drag toolbox
IF NOT SelectMajorsThread
  GLO:DropThread = THREAD()
  GLO:DropControl = ?STU:Major
  SelectMajorsThread = START(SelectMajors)
  GLO:ThreadRef &= SelectMajorsThread
END
[END]
WHEN 'Drop'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Receive dropped data and close drag toolbox
STU:Major = DROPID()
DISPLAY
POST(EVENT:CloseWindow,,SelectMajorsThread)
SelectMajorsThread = 0
DO RefreshWindow
PRESSKEY(TabKey)
[END]
WHEN 'Selected'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Call drag toolbox
IF OriginalRequest = InsertRecord AND NOT STU:Major
  GLO:DropThread = THREAD()
  GLO:DropControl = ?STU:Major
  SelectMajorsThread = START(SelectMajors)
  GLO:ThreadRef &= SelectMajorsThread
END
[END]
[END]
WHEN '?Button8'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Call drag toolbox
POST(EVENT:AlertKey,?STU:Major)
[END]
[END]
[END]
