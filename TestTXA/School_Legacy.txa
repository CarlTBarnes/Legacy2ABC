[INSTANCES]
WHEN 'CloseWindow'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Close drag toolbox
IF SelectMajorsThread
  POST(EVENT:CloseWindow,,SelectMajorsThread)
END
[END]
[END]
EMBED %ControlPreEventHandling
[INSTANCES]
WHEN '?CurrentTab'
[INSTANCES]
WHEN 'NewSelection'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
  !Code to assign button control based upon current tab selection
  CASE CHOICE(?CurrentTab)
  OF 1
    DO FORM::AssignButtons
  OF 2
    DO BRW2::AssignButtons
  END
[END]
[END]
WHEN '?Tea:Department'
[INSTANCES]
WHEN 'AlertKey'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Call drag toolbox
IF NOT SelectMajorsThread
  GLO:DropThread = THREAD()
  GLO:DropControl = ?Tea:Department
  SelectMajorsThread = START(SelectMajors)
  GLO:ThreadRef &= SelectMajorsThread
END
[END]
WHEN 'Drop'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Receive dropped data and close drag toolbox
Tea:Department = DROPID()
DISPLAY
POST(EVENT:CloseWindow,,SelectMajorsThread)
SelectMajorsThread = 0
DO RefreshWindow
PRESSKEY(TabKey)
PRESSKEY(TabKey)
[END]
WHEN 'Selected'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Call drag toolbox
IF OriginalRequest = InsertRecord AND NOT Tea:Department
  GLO:DropThread = THREAD()
  GLO:DropControl = ?Tea:Department
  SelectMajorsThread = START(SelectMajors)
  GLO:ThreadRef &= SelectMajorsThread
END
[END]
[END]
WHEN '?Button7'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Call drag toolbox
POST(EVENT:AlertKey,?Tea:Department)
[END]
[END]
[END]
[END]
