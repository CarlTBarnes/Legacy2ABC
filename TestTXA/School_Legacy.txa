[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Call drag toolbox
IF NOT SelectCoursesThread
  GLO:DropThread = THREAD()
  GLO:DropControl = ?CLA:CourseNumber
  SelectCoursesThread = START(SelectCourses)
  GLO:ThreadRef &= SelectCoursesThread
END
[END]
