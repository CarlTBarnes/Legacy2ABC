SelectLabelPaper ROUTINE
!Allow Run-time Specification of Label Paper
  GlobalRequest = SelectRecord
  BrowseLabels                 
  IF GlobalResponse = RequestCancelled
    DO ProcedureReturn
  END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
GetReportFilterAndKey  ROUTINE
!Allow Run-time Record Selection and Sort Specification
  IF GetNewFilterAndKey
    SelectNSort(CTRL:ReportFilter,CTRL:ReportKey,CommitteeList)
    IF GlobalResponse = RequestCancelled
      DO ProcedureReturn
    END
  END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
ResizeTheReport ROUTINE 
  !=============================================================
  !Modify report to fit specified label paper.
  !Assumes Report Measurement Unit is 1/1000 of an inch.
  SETTARGET(report)  !Make report the target for property assignments
  !=============================================================
  !Define the REPORT's AT attribute (ie detail print area).
  !The detail print area should be the size of the page.
  report{PROP:Width}  = LAB:PageWidth  * 1000
  report{PROP:Height} = LAB:PageHeight * 1000
  !Adjust the margins to center the address text within each label.
  report{PROP:Xpos}   = LAB:LeftMargin * 1000
  report{PROP:Ypos}   = LAB:TopMargin  * 1000
  !=============================================================
  !Define the DETAIL's AT attribute.
  !The DETAIL should be the size of each individual label.
  ?detail{PROP:Width}  = LAB:LabelWidth  * 1000
  ?detail{PROP:Height} = LAB:LabelHeight * 1000
  !=============================================================
  !Adjust font size and vertical position of the report fields.
  LOOP ReportField = FIRSTFIELD() TO LASTFIELD() !Process all the report fields.
    ReportField{PROP:Fontsize} = LAB:FontSize    !Assign font size from label file.
    IF ReportField > FIRSTFIELD()                !Skip the first field, reposition the rest...
      ReportField{PROP:Ypos} = BottomOfPreviousField
    END 
    BottomOfPreviousField = ReportField{PROP:Ypos}+(16*LAB:FontSize) !Calculate Y coord
  END
  SETTARGET()  !reset the default target to the active window
                EXIT
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
RemoveExtraSpaces ROUTINE
!Remove extra spaces from names and addresses
  ClippedName=CLIP(MBR:FirstName)&' '&MBR:LastName
  CityStateZIP=CLIP(MBR:City)&', '&MBR:State&' '&MBR:ZIPCode
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
SuppressBlankLines ROUTINE
  IF MBR:Address2
    LabelField3 = MBR:Address2
    LabelField4 = CityStateZip
  ELSE 
    LabelField3 = CityStateZip
    LabelField4 = ''
  END
[END]
