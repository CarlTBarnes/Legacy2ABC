LoadMemberFilter ROUTINE
  IF CTRL:ReportFilter
    IF INSTRING('MBR:Active',CTRL:ReportFilter,1)
      MemberFilter = CTRL:ReportFilter[INSTRING('MBR:Active',CTRL:ReportFilter,1):12]
    ELSE 
      MemberFilter = 'Both'
    END
  ELSE 
    MemberFilter = 'Both'
  END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
MarkCommitteAndTypeList ROUTINE
  
!Collect Marked Committees
  LOOP FLD5::LoopIndex = 1 TO RECORDS(Queue:FileDrop:1)
    GET(Queue:FileDrop:1,FLD5::LoopIndex)
    IF INSTRING(CLIP(FLD5::COM:Committee),CTRL:ReportFilter,1)
       FLD5::Marked = True
       PUT(Queue:FileDrop:1)
    END
  END

!Collect Marked Types
  LOOP FLD6::LoopIndex = 1 TO RECORDS(Queue:FileDrop:3)
    GET(Queue:FileDrop:3,FLD6::LoopIndex)
    IF INSTRING(CLIP(FLD6::TYP:MemberType),CTRL:ReportFilter,1)
       FLD6::Marked = True
       PUT(Queue:FileDrop:3)
    END
  END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
BuildFiltersAndKey ROUTINE

!Collect Marked Committees 
  CommitteeFilter = ''
  LOOP FLD5::LoopIndex = 1 TO RECORDS(Queue:FileDrop:1)
    GET(Queue:FileDrop:1,FLD5::LoopIndex)
    IF FLD5::Marked
      IF CommitteeFilter
        CommitteeFilter = CLIP(CommitteeFilter)&|
        ' OR CMBRS:Committee='''&CLIP(FLD5::COM:Committee)&''''
      ELSE
        CommitteeFilter = '(CMBRS:Committee='''&CLIP(FLD5::COM:Committee)&''''
      END
    END
  END

!Collect Marked MemberTypes
  TypeFilter = '' 
  AllTypesAreMarked=True
  LOOP FLD6::LoopIndex = 1 TO RECORDS(Queue:FileDrop:3)
    GET(Queue:FileDrop:3,FLD6::LoopIndex)
    IF FLD6::Marked
      IF TypeFilter
        TypeFilter = CLIP(TypeFilter)&|
        ' OR MBR:MemberType='''&CLIP(FLD6::TYP:MemberType)&''''
      ELSE
        TypeFilter = '(MBR:MemberType='''&CLIP(FLD6::TYP:MemberType)&''''
      END
    ELSE
      AllTypesAreMarked=False
    END
  END
  IF AllTypesAreMarked
    TypeFilter = ''
  END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
BuildGlobalReportFilter ROUTINE
 
!Set Member filter
  IF MemberFilter = 'Both'
    CTRL:ReportFilter = ''
  ELSE
    CTRL:ReportFilter = CLIP(MemberFilter)
  END

!Add the member type filter
  IF TypeFilter
    IF CTRL:ReportFilter
      CTRL:ReportFilter = CLIP(CTRL:ReportFilter) &' AND '& CLIP(TypeFilter)&')'
    ELSE
      CTRL:ReportFilter = CLIP(TypeFilter)&')'
    END
  END

!add the committee filter
  IF CommitteeFilter
    IF CTRL:ReportFilter
      CTRL:ReportFilter = CLIP(CTRL:ReportFilter) &' AND '& CLIP(CommitteeFilter)&')'
    ELSE
      CTRL:ReportFilter = CLIP(CommitteeFilter)&')'
    END
  END
[END]
[END]
