LoadMemberFilter ROUTINE
  IF ReportFilter
    IF INSTRING('MBR:Active',ReportFilter,1)
      LocalMemberFilter = ReportFilter[INSTRING('MBR:Active',ReportFilter,1):12]
    ELSE 
      LocalMemberFilter = 'Both'
    END
  ELSE 
    LocalMemberFilter = 'Both'
  END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
MarkCommitteAndTypeList ROUTINE
!Collect Marked Committees
  IF FilterByCommittee
    LOOP FLD2::LoopIndex = 1 TO RECORDS(Queue:FileDrop)
      GET(Queue:FileDrop,FLD2::LoopIndex)
      IF INSTRING(CLIP(FLD2::COM:Committee),ReportFilter,1)
         FLD2::Marked = True
         PUT(Queue:FileDrop)
      END
    END
  ELSE
    DISABLE(?COM:Committee)
  END

!Collect Marked Types
  LOOP FLD3::LoopIndex = 1 TO RECORDS(Queue:FileDrop:1)
    GET(Queue:FileDrop:1,FLD3::LoopIndex)
    IF INSTRING(CLIP(FLD3::TYP:MemberType),ReportFilter,1)
       FLD3::Marked = True
       PUT(Queue:FileDrop:1)
    END
  END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
BuildFiltersAndKey ROUTINE

!Collect Marked Committees
  LocalCommitteeFilter = ''
  LOOP FLD2::LoopIndex = 1 TO RECORDS(Queue:FileDrop)
    GET(Queue:FileDrop,FLD2::LoopIndex)
    IF FLD2::Marked
      IF LocalCommitteeFilter
        LocalCommitteeFilter = CLIP(LocalCommitteeFilter)&|
        ' OR CMBRS:Committee='''&CLIP(FLD2::COM:Committee)&''''
      ELSE
        LocalCommitteeFilter = '(CMBRS:Committee='''&CLIP(FLD2::COM:Committee)&''''
      END
    END
  END

!Collect Marked MemberTypes
  LocalTypeFilter = '' 
  AllTypesAreMarked=True
  LOOP FLD3::LoopIndex = 1 TO RECORDS(Queue:FileDrop:1)
    GET(Queue:FileDrop:1,FLD3::LoopIndex)
    IF FLD3::Marked
      IF LocalTypeFilter
        LocalTypeFilter = CLIP(LocalTypeFilter)&|
        ' OR MBR:MemberType='''&CLIP(FLD3::TYP:MemberType)&''''
      ELSE
        LocalTypeFilter = '(MBR:MemberType='''&CLIP(FLD3::TYP:MemberType)&''''
      END
    ELSE
      AllTypesAreMarked=False
    END
  END
  IF AllTypesAreMarked
    LocalTypeFilter = ''
  END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
BuildReportFilter ROUTINE

!Set Member filter
  IF LocalMemberFilter = 'Both' 
    ReportFilter=''
  ELSE
    ReportFilter = CLIP(LocalMemberFilter)
  END
!Add the member type filter
  IF LocalTypeFilter
    IF ReportFilter
      ReportFilter = CLIP(ReportFilter) &' AND '& CLIP(LocalTypeFilter)&')'
    ELSE
      ReportFilter = CLIP(LocalTypeFilter)&')'
    END
  END
!Add the committee filter
  IF LocalCommitteeFilter
    IF ReportFilter
      ReportFilter = CLIP(ReportFilter) &' AND '& CLIP(LocalCommitteeFilter)&')'
    ELSE
      ReportFilter = CLIP(LocalCommitteeFilter)&')'
    END
  END
[SOURCE]
