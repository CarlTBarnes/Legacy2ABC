[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!Let this procedure start only once
IF MainInstance
  BrowseMembersThread=0
  !If we shut down BrowseMembers, we also want
  !to make sure the CommitteeAssignments toolbox gains
  !focus so it doesn't lay there like a dead window.
  !The CommitteeAssignments window detects
  !whether the BrowseMembers window is active or not
  !and becomes a toolbox or not depending thereon.
  IF CommitteeAssignmentsThread
    POST(EVENT:GainFocus,,CommitteeAssignmentsThread)
  END
  DISABLE(TransactionsButton)
END
[END]
EMBED %ProcedureRoutines
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
SetBrowseFilter ROUTINE
  CASE CTRL:ReportFilter[INSTRING('MBR:Active',CTRL:ReportFilter,1):12]
  OF 'MBR:Active=1'    ;BrowseFilter = 'Active'
  OF 'MBR:Active=0'    ;BrowseFilter = 'Inactive'
  ELSE                 ;BrowseFilter = 'All'
  END
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
DragFromMemberList   ROUTINE
! This Routine Gets the record for the bighlighted item in the List
! and puts it in the CLIPBOARD so it can be dropped elsewhere.
  GET(QUEUE:BROWSE:1,CHOICE(?MembersList))
  SETCLIPBOARD(BRW1::MBR:MemberNumber)
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
EnableDisableDragNDrop ROUTINE
!There is drag and drop capability between this procedure
!and the CommitteeAssignments procedure. When both are
!open, Drag is enabled for this procedure.
!However, when CommitteeAssignments is closed,
!Drag capability should be disabled.
  IF LocalCommitteeAssignmentsThread = CommitteeAssignmentsThread   !if committee window is unchanged
  ELSE                                                              !if committee window is changed
    LocalCommitteeAssignmentsThread = CommitteeAssignmentsThread    !save its new status
    IF CommitteeAssignmentsThread                                   !if committe window is open
      ?MembersList{PROP:Dragid,1}='Member'                              !enable drag n drop
      ?MembersList{PROP:ToolTip}='Drag from member list onto a committee'
    ELSE                                                           !if committee window is closed
      ?MembersList{PROP:Dragid,1}=''                                  !Disable drag n drop
      ?MembersList{PROP:ToolTip}=''
    END
  END
[END]
[END]
