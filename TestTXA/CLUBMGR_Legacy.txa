window  WINDOW('Select and Sort report records'),AT(,,202,127),FONT('MS Sans Serif',8,COLOR:Black, |
          FONT:regular),RESIZE,ICON('CLUBMGR.ICO'),GRAY,IMM,MAX,MDI,HLP('~SelectNSort')
          GROUP('Select Members to include...'),AT(2,1,195,73),USE(?SelectGroup),BOXED,#ORIG(?Group1)
            OPTION('Status'),AT(7,9,50,62),USE(LocalMemberFilter),BOXED,MSG('Include ac' & |
              'tive, inactive, or all members'),#ORIG(?Option1)
              RADIO('Active'),AT(13,26,38,10),USE(?Option1:Radio1),VALUE('MBR:Active=1'),#ORIG(?Option1:Radio1)
              RADIO('Inactive'),AT(13,40),USE(?Option1:Radio2),VALUE('MBR:Active=0'),#ORIG(?Option1:Radio2)
              RADIO('Both'),AT(13,54),USE(?Option1:Radio3),VALUE('Both'),#ORIG(?Option1:Radio3)
            END
            LIST,AT(60,13,63,57),USE(COM:Committee),VSCROLL,FORMAT('80L~Committee~L(1)@s20@'), |
              FROM(Queue:FileDrop),MARK(FLD2::Marked),MSG('Include only the members on ' & |
              'the marked committees'),TIP('Include only the members on the marked committees'), |
              #FIELDS(COM:Committee),#ORIG(COM:Committee),#SEQ(2)
            LIST,AT(127,13,63,57),USE(TYP:MemberType),VSCROLL,FORMAT('60L~Member Type~L(1)@s15@'), |
              FROM(Queue:FileDrop:1),MARK(FLD3::Marked),MSG('Include only the members w' & |
              'ith the marked types'),TIP('Include only the members with the marked types'), |
              #FIELDS(TYP:MemberType),#ORIG(TYP:MemberType),#SEQ(3)
          END
          OPTION('Sort by...'),AT(2,74,195,28),USE(ReportKey),BOXED,MSG('Sort by name, ' & |
            'member type, or ZIP Code'),#ORIG(?Option2)
            RADIO('Member Name'),AT(9,85),USE(?Option2:Radio1),VALUE('MBR:NameKey'),#ORIG(?Option2:Radio1)
            RADIO('ZIP Code'),AT(147,85),USE(?Option2:Radio2),VALUE('MBR:ZIPCodeKey'),#ORIG(?Option2:Radio2)
            RADIO('Member Type'),AT(79,85),USE(?Option2:Radio3),VALUE('MBR:TypeKey'),#ORIG(?Option2:Radio3)
          END
          BUTTON('Cancel'),AT(104,107,45,14),USE(?Cancel),#ORIG(?Button3)
          BUTTON('OK'),AT(56,107,45,14),USE(?OK),#ORIG(?Close),#SEQ(1)
          BUTTON('Help'),AT(153,107,45,14),USE(?Help),STD(STD:Help),#ORIG(?Button2)
        END

