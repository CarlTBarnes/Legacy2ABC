DropMemberOnCommitteeList ROUTINE
  LOOP i# = ?RELTREE{PROPLIST:MouseUpRow} TO 1 BY -1
    GET(Queue:RelTree,i#)
  UNTIL (REL1::Level < CommitteeMemberLevel)
  CMbrs:Committee=CLIP(REL1::Display)
  CMbrs:MemberNumber=CLIPBOARD()
  GET(CommitteeMembers,0)
  IF DUPLICATE(CMbrs:KeyAssignment)
    MESSAGE('This person is already on the committee','',ICON:Exclamation,BUTTON:OK,) 
    GET(CommitteeMembers,CMbrs:KeyAssignment)
  ELSE
    ADD(CommitteeMembers)
  END 
  REL1::NewItemPosition = POSITION(CommitteeMembers)
  REL1::NewItemLevel = CommitteeMemberLevel
  DO REL1::RefreshTree
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
EnableDisableEditButton  ROUTINE
!Changes to CommitteeMembers are only allowed with
!Drag and Drop. Therefore, disable the
!change button when a member is selected.
!Conversely, enable the change button if a
!committee is selected.
  REL1::SavePointer=POINTER(Queue:RelTree)
  GET(Queue:RelTree,CHOICE(?RELTREE))
  IF REL1::Level = CommitteeMemberLevel
    IF ?Change{PROP:Disable}
    ELSE
      DISABLE(?Change)
    END
  ELSE 
    IF ?Change{PROP:Disable}
      ENABLE(?Change)
    ELSE
    END
  END
  GET(Queue:RelTree,REL1::SavePointer)
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
EnableDisableToolboxBehavior ROUTINE
!There is drag and drop capability between this procedure
!and the BrowseMembers procedure. When both procedures are
!open, this procedure becomes a toolbox (always on top)
!so that it doesn't disappear behind the bigger
!BrowseMembers window during a drag and drop operation.
!However, when BrowseMembers is closed, this procedure
!should revert back to normal nontoolbox behavior.

  IF LocalBrowseMembersThread = BrowseMembersThread  !If browsemembers thread is unchanged
    IF BrowseMembersThread                             !if browsemembers is open
      IF (UpdateMembersIsOpen OR BrowseTransactionsIsOpen) !if child window is open
        ?RELTREE{PROP:Dragid,1}=''                           !disable drag n drop
        ?RELTREE{PROP:ToolTip}=''
      ELSE                                                 !if no child window
        ?RELTREE{PROP:Dragid,1}='CommitteeMbr'                !enable drag n drop
        ?RELTREE{PROP:ToolTip}='Drag committe member off of committee list'
      END
    END                                              
  ELSE                                               !if browsemembers thread changed
    LocalBrowseMembersThread = BrowseMembersThread     !store its new status (open or closed)
    IF BrowseMembersThread                             !if open
      window{PROP:Toolbox}=True                          !enable drag n drop and toolbox
      ?RELTREE{PROP:Dragid,1}='CommitteeMbr'
      ?RELTREE{PROP:ToolTip}='Drag committe member off of committee list' 
    ELSE                                               !if closed
      window{PROP:Toolbox}=''                             !disable drag n drop and toolbox
      ?RELTREE{PROP:Dragid,1}=''
      ?RELTREE{PROP:ToolTip}=''
    END
  END
[END]
